{
    "name": "swayok/laravel-blade-directives",
    "description": "Additional Blade directives for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippov Alexander"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "blade",
        "dotJs",
        "partials"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-0": {
            "LaravelBladeDirectives": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBladeDirectives\\LaravelBladeDirectivesServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/swayok/laravel-blade-directives/issues"
    }
}
